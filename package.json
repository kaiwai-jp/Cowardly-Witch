{
  "name": "Cowardly-Witch",
  "version": "7.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint .",
    "format": "prettier-eslint --write '**/*.js'",
    "minify:css": "uglifycss public/stylesheets/style.css --output public/stylesheets/style.css",
    "build": "npm-run-all -s build:prod minify:css",
    "build:prod": "cross-env MODE=production webpack",
    "build:dev": "cross-env MODE=development webpack",
    "watch": "cross-env MODE=development webpack --watch",
    "serve": "webpack-dev-server"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "keywords": [
    "yui540",
    "yui540.graphics",
    "css animation"
  ],
  "repository": "git@github.com:yui540/Cowardly-Witch.git",
  "author": "yui540 <tabletennis540@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "eslint": "5.1.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "husky": "0.14.3",
    "import-glob-loader": "1.1.0",
    "lint-staged": "7.2.0",
    "node-sass": "7.0.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.13.7",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "sass-loader": "7.0.3",
    "uglifycss": "0.0.29",
    "url-loader": "1.0.1",
    "webpack": "4.16.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.11"
  },
  "dependencies": {
    "react": "16.4.1",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0"
  }
}
